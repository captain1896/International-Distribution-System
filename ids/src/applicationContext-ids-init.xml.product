          <value>classpath:idsconfig/agoda/agodaconfig.properties</value>
          <value>classpath:idsconfig/genares/genaresconfig.properties</value>
          <value>classpath:idsconfig/expedia/expediaconfig.properties</value>
          <value>classpath:idsconfig/bookingcom/bookingcomconfig.properties</value>
          <value>classpath:idsconfig/asiarooms/asiaroomsconfig.properties</value>
          <value>classpath:idsconfig/venere/venereconfig.properties</value>
      </list>
    </property>
  </bean>
</beans>  